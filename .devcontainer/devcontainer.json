{
  "name": "caa Dev Container",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "remoteEnv": {
    "SOPS_AGE_KEY_FILE": "/workspaces/caa2/config/key.txt"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "opentofu.vscode-opentofu"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "fish"
      }
    }
  },
  "remoteUser": "root"
}